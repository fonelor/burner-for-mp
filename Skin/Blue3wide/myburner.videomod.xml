      <control>
      	<description>BitRate</description>
	<type>button</type>
      	<id>2</id>
      	<label>Quality: SP</label>
        <onleft>7</onleft>
        <onright>7</onright>
        <onup>6</onup>
        <ondown>3</ondown>
      </control>
      <control>
        <description>Main Menu</description>
        <type>button</type>
        <id>3</id>
