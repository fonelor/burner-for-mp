      <textureNoFocus>#background_menu</textureNoFocus>
      <onright>2</onright>
      <onleft>2</onleft>
      <onup>8</onup>
      <ondown>8</ondown>
    </control>
    <control>
      <description>Show Names</description>
      <type>listcontrol</type>
      <id>9</id>
      <posX>480</posX>
      <posY>515</posY>
      <height>250</height>
      <onright>2</onright>
      <onleft>2</onleft>
      <onup>7</onup>
      <ondown>7</ondown>
