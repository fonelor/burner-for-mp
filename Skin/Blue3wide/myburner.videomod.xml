        <label>Play Show</label>
        <onleft>7</onleft>
        <onright>7</onright>
        <onup>3</onup>
        <ondown>5</ondown>
      </control>
      <control>
        <description>Episodes</description>
        <type>button</type>
        <id>5</id>
