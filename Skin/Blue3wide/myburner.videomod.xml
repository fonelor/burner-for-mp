        <onleft>7</onleft>
        <onright>7</onright>
        <onup>4</onup>
        <ondown>6</ondown>
      </control>
      <control>
        <description>Disk name</description>
        <type>button</type>
        <id>6</id>
        <label>Disk Name</label>
        <onleft>7</onleft>
        <onright>7</onright>
        <onup>5</onup>
        <ondown>7</ondown>
      </control>
      <control>
        <description>Done</description>
        <type>button</type>
        <id>7</id>
        <label>Done</label>
        <onleft>8</onleft>
        <onright>8</onright>
        <onup>6</onup>
        <ondown>8</ondown>
      </control>
    </control>
    <control>
      <description>BackGroundImage</description>
      <type>button</type>
      <id>8</id>
      <posX>480</posX>
      <posY>121</posY>
      <height>384</height>
